GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/LumberjackConsole" "${PODS_ROOT}/Headers/Public/NBUCore"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/LumberjackConsole" -isystem "${PODS_ROOT}/Headers/Public/NBUCore"
OTHER_LDFLAGS = -ObjC -l"Pods-CocoaLumberjack" -l"Pods-LumberjackConsole" -l"Pods-NBUCore"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods